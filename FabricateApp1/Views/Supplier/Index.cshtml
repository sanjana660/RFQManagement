@model IEnumerable<FabricateApp1.Models.Supplier>
@{
    ViewBag.Title = "Supplier Index";
    Layout= "~/Views/Shared/_Afterlogin.cshtml";
}
<center>
<h2>Suppliers</h2>
<p>
    @Html.ActionLink("Create New Supplier","AddSupplier") |
    @Html.ActionLink("Update Supplier Details","Update") |
    @Html.ActionLink("Get Supplier Of Part","GetSupplierOfPart")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.phone)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.location)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.feedback)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
<td>
    @Html.DisplayFor(modelItem => item.name)
</td>
<td>
    @Html.DisplayFor(modelItem => item.email)
</td>
<td>
    @Html.DisplayFor(modelItem => item.phone)
</td>
<td>
    @Html.DisplayFor(modelItem => item.location)
</td>
<td>
    @Html.DisplayFor(modelItem => item.feedback)
</td>
</tr>
}
</table>
</center>
